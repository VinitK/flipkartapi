<html lang="en">
<head>
    <%- include('include/head', {title: title}); %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>s
    <script type="text/javascript" src="/js/flipkart.js"></script>
</head>
<body>
    <div class="jumbotron text-center">
        <h1 class="display-4"><%=title%></h1>
        <h2 class="font-weight-light"><%=message%></h2>
        <div class="row justify-content-md-center">
            <div class="input-group mt-4 col-sm-4">
                <input type="text" class="form-control" placeholder="iPhone X, Laptops, Books, etc.">
                <div class="input-group-append">
                    <button onclick="search(this)" class="btn btn-outline-primary" type="button">Search</button>
                </div>
            </div>
        </div>
    </div>
</body>
</html>